{
  "name": "hellome.ai",
  "version": "0.1.1",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "analyze": "BUNDLE_ANALYZE=both next build",
    "typecheck:tsc": "tsc --noemit",
    "typecheck": "run-s --silent --print-name typecheck:*",
    "lint:cspell": "cspell --no-progress --cache --cache-location .cspellcache ./",
    "lint:next": "next lint --cache-location .eslintcache",
    "lint": "run-s --silent --print-name lint:*",
    "lint-fix:next": "next lint --cache-location .eslintcache --fix",
    "lint.fix": "run-s --silent --print-name lint:cspell lint-fix:*",
    "format:prettier": "prettier --loglevel warn --cache --cache-location=.prettiercache --write ./",
    "format": "run-s --silent --print-name format:*",
    "test": "jest",
    "test.ci": "jest --ci",
    "test.fix": "jest --updateSnapshot",
    "test.watch": "jest --watch",
    "ci": "run-s --silent typecheck lint test.ci",
    "fix": "run-s --silent format typecheck lint.fix test.fix",
    "find:unused": "npx next-unused"
  },
  "sideEffects": false,
  "dependencies": {
    "@next/font": "13.1.6",
    "@stdlib/datasets-female-first-names-en": "^0.0.9",
    "@stdlib/datasets-male-first-names-en": "^0.0.9",
    "@vercel/analytics": "^0.1.8",
    "clsx": "^1.2.1",
    "flowbite": "^1.6.3",
    "flowbite-react": "^0.3.8",
    "lodash": "^4.17.21",
    "next": "13.1.6",
    "nextjs-google-analytics": "^2.3.0",
    "postcss-nesting": "^11.2.0",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "dependencies.todo": {
    "flowbite": "^1.6.3",
    "flowbite-react": "^0.3.8"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^13.1.6",
    "@tailwindcss/forms": "^0.5.3",
    "@testing-library/dom": "^8.20.0",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@testing-library/user-event": "14.4.3",
    "@types/lodash": "^4.14.191",
    "@types/node": "18.13.0",
    "@types/react": "18.0.27",
    "@types/react-dom": "18.0.10",
    "@types/testing-library__jest-dom": "5.14.5",
    "autoprefixer": "^10.4.13",
    "cspell": "^6.25.0",
    "eslint": "8.33.0",
    "eslint-config-next": "13.1.6",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-testing-library": "^5.10.1",
    "jest": "29.4.2",
    "jest-environment-jsdom": "29.4.2",
    "lint-staged": "^13.1.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-preset-env": "^8.0.1",
    "prettier": "^2.8.4",
    "tailwindcss": "^3.2.6",
    "ts-jest": "^29.0.5",
    "typescript": "4.9.5"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "eslint",
      "prettier --write",
      "git add"
    ],
    "**/*.{md,mdx,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "browserslist": [
    "chrome >= 90",
    "edge >= 90",
    "firefox >= 100",
    "android >= 90",
    "safari >= 15"
  ]
}
