  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "eslint",
      "prettier --write",
      "git add"
    ],
    "**/*.{md,mdx,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "browserslist": [
    "defaults and supports es6-module",
    "maintained node versions"
  ],
  "next-unused": {
    "alias": {
      "@": "./"
    },
    "include": [
      "components",
      "public",
      "styles"
    ],
    "exclude": [
      "public/locales",
      "public/sw.js",
      "public/fallback-*.js",
      "public/workbox-*.js"
    ],
    "entrypoints": []
  }
