{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "target": "es5",
    "lib": ["es5", "dom"],
    "noEmit": true,
    "sourceMap": false,
    "noUnusedLocals": false,
    "isolatedModules": false,
    "noUnusedParameters": false,
    "types": [
      "node",
      "cypress",
      "cypress-wait-until",
      "cypress-react-selector",
      "@testing-library/cypress"
    ]
  },
  "include": ["**/*.ts", ".eslintrc.js"],
  "exclude": []
}
