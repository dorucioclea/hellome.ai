{
  "name": "hellome.ai",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "analyze": "BUNDLE_ANALYZE=both next build",
    "check-types": "tsc --noemit",
    "spellcheck": "cspell . --no-progress",
    "lint": "npm run check-types && next lint && npm run spellcheck",
    "fix": "pnpm run prettier-fix && pnpm run lint",
    "prettier-fix": "prettier --write .",
    "find:unused": "npx next-unused"
  },
  "sideEffects": false,
  "dependencies": {
    "@next/font": "13.1.6",
    "@stdlib/datasets-female-first-names-en": "^0.0.9",
    "@stdlib/datasets-male-first-names-en": "^0.0.9",
    "@vercel/analytics": "^0.1.8",
    "clsx": "^1.2.1",
    "lodash": "^4.17.21",
    "next": "13.1.6",
    "nextjs-google-analytics": "^2.3.0",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "dependencies.todo": {
    "flowbite": "^1.6.3",
    "flowbite-react": "^0.3.8"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^13.1.6",
    "@types/lodash": "^4.14.190",
    "@types/node": "18.13.0",
    "@types/react": "18.0.27",
    "@types/react-dom": "18.0.10",
    "cspell": "^6.22.0",
    "eslint": "8.33.0",
    "eslint-config-next": "13.1.6",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-json": "^3.1.0",
    "lint-staged": "^13.0.3",
    "prettier": "^2.5.1",
    "typescript": "4.9.5"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "eslint",
      "prettier --write",
      "git add"
    ],
    "**/*.{md,mdx,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "browserslist": [
    "chrome >= 90",
    "edge >= 90",
    "firefox >= 100",
    "android >= 90",
    "safari >= 15"
  ]
}
