{
  "name": "hellome.ai",
  "version": "0.3.1",
  "private": true,
  "scripts": {
    "dev": "nodemon --config next.nodemon.json --exec next dev",
    "dev.inspect": "nodemon --config next.nodemon.json --exec env NODE_OPTIONS='--inspect' next dev",
    "prebuild": "yarn gen.routes",
    "build": "next build",
    "postbuild": "next-sitemap",
    "start": "next start",
    "build.ci": "env NODE_ENV=test yarn build",
    "start.ci": "env NODE_ENV=test yarn start",
    "analyze": "env ANALYZE=true next build",
    "analyze.source.html": "source-map-explorer .next/static/**/*.js --html result.html && open result.html",
    "analyze.source.tsv": "source-map-explorer .next/static/**/*.js --tsv | column -t -s $'\t' | bat -l tsv",
    "analyze.source.json": "source-map-explorer .next/static/**/*.js --json",
    "gen.routes": "npx nextjs-routes",
    "gen.prisma": "npx prisma generate",
    "mailing.dev": "npx mailing",
    "mailing.build": "npx mailing server build",
    "mailing.start": "npx mailing server start",
    "typecheck:tsc": "tsc --noemit --project tsconfig.json",
    "typecheck:tsc.cypress": "tsc --noemit --project cypress/tsconfig.json",
    "pretypecheck": "yarn gen.routes",
    "typecheck": "run-s --silent --print-name typecheck:*",
    "lint:cspell": "cspell lint --no-progress --cache --cache-location .cspellcache ./ || true",
    "lint:next": "next lint --cache-location .eslintcache",
    "lint:cypress": "eslint --config cypress/.eslintrc.js",
    "lint:browserslist": "npx browserslist-lint",
    "lint": "run-s --silent --print-name lint:*",
    "lint-fix:next": "next lint --cache-location .eslintcache --fix",
    "lint-fix:cypress": "eslint --config cypress/.eslintrc.js --fix",
    "lint.fix": "run-s --silent --print-name lint:browserslist lint:cspell lint-fix:*",
    "format:prettier": "prettier --loglevel warn --cache --cache-location=.prettiercache --write ./",
    "format": "run-s --silent --print-name format:*",
    "clean:cspell": "rimraf .cspellcache",
    "clean:eslint": "rimraf eslintcache",
    "clean:next": "rimraf .next .swc public/fallback-*.js public/sitemap*.xml public/sw.js public/workbox-*.js",
    "clean:node_modules": "rimraf node_modules",
    "clean:prettier": "rimraf .prettiercache",
    "clean:tsc": "rimraf tsconfig.tsbuildinfo",
    "_clean:nextjs-routes": "rimraf nextjs-routes.d.ts",
    "clean:yarn": "rimraf yarn-error.log",
    "clean": "run-p --silent --print-name clean:** && yarn install",
    "test": "jest",
    "test.ci": "jest --ci",
    "test.fix": "jest --updateSnapshot",
    "test.watch": "jest --watch",
    "e2e": "yarn build.ci && start-server-and-test 'yarn start.ci' http://127.0.0.1:3000 'yarn cy.run'",
    "cy.run": "env NODE_OPTIONS=--openssl-legacy-provider yarn cypress run --config-file cypress/cypress.config.ts",
    "cy.open": "env NODE_OPTIONS=--openssl-legacy-provider yarn cypress open --config-file cypress/cypress.config.ts",
    "ci": "run-s --silent typecheck lint test.ci",
    "fix": "run-s --silent format typecheck lint.fix test.fix",
    "up": "yarn upgrade-interactive --latest",
    "unused": "next-unused"
  },
  "sideEffects": false,
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.4.0",
    "@fortawesome/free-brands-svg-icons": "^6.4.0",
    "@fortawesome/free-solid-svg-icons": "^6.4.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@headlessui/react": "^1.7.14",
    "@headlessui/tailwindcss": "^0.1.3",
    "@heroicons/react": "^2.0.17",
    "@hookform/error-message": "^2.0.1",
    "@next/bundle-analyzer": "^13.3.0",
    "@stdlib/datasets-female-first-names-en": "^0.0.9",
    "@stdlib/datasets-male-first-names-en": "^0.0.9",
    "@vercel/analytics": "^0.1.11",
    "@vercel/og": "^0.5.2",
    "classnames": "^2.3.2",
    "clsx": "^1.2.1",
    "crypto": "^1.0.1",
    "date-fns": "^2.29.3",
    "dotenv": "^16.0.3",
    "encoding": "^0.1.13",
    "flowbite": "^1.6.5",
